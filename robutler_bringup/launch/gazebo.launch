<launch>
  <arg name="model" default="$(env TURTLEBOT3_MODEL)" doc="model type [burger, waffle, waffle_pi]"/>
  <arg name="x_pos" default="-2.0"/>
  <arg name="y_pos" default="-0.5"/>
  <arg name="z_pos" default="0.0"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <!-- lancamento pelo pckg turtlebot3_gazebo -->
    <!-- <arg name="world_name" value="$(find turtlebot3_gazebo)/worlds/psr_apartment.world"/>  -->
    <!-- lancamento pelo pckg psr_apartment -->
    <arg name="world_name" value="$(find psr_apartment_description)/worlds/psr_apartment.world"/>
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>

</launch>